<%= form_for @staff do |f| %>
    <% if @staff.errors.any? %>
    <div id="error_explanation">
        <h2>Errors:</h2>
        <ul>
            <% @staff.errors.full_messages.each do |message| %>
            <li><%= message %></li>
            <%end%>
        </ul>
    </div>
    <%end%>

    <%= f.label :name, "Name" %>
    <%= f.text_field :name %> <br>

    <%= f.label :bio, "Bio" %>
    <%= f.text_area :bio %>

    <%= f.submit 'Submit Staff'%>
<% end %>